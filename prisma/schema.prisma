// schema.prisma

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model AllowedAddress {
  address   String  @id
  quantityAllowed  Int    @default(0)
  isLocked  Boolean @default(false)
  uuid      String?
}

model MintedTokens {
  tokenId   Int    @unique
  toAddress   String
  quantity  Int
  createdAt DateTime @default(now())
}